    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome 8081",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8081",
            "webRoot": "${workspaceFolder}"
        },

        {
      "name": "Debug JS in Expo App",
      "program": "${workspaceFolder}/.vscode/.react",
      "type": "reactnative",
      "request": "attach",
      "platform": "android",
      "sourceMaps": true
    }
    ]
}